           prefHeight="27.0" prefWidth="50.0" text="Send" AnchorPane.leftAnchor="375.0" AnchorPane.rightAnchor="174.6"/>
   <Button fx:id="startGame" layoutX="495.0" layoutY="360.0" mnemonicParsing="false" onAction="#startGame"
           text="START GAME" AnchorPane.leftAnchor="495.0" AnchorPane.rightAnchor="18.599999999999994"/>
